<h1>Chart</h1>
<!--<script src="http://code.highcharts.com/modules/exporting.js"></script>-->


<div id="Bar_Chart_div" style = "padding-left: 10%; width: 80%" >
      <%= high_chart("chart", @chart) %>
</div>

<table border="1" style="width:20%">
    <% for i in 0..@tableHeadings.length-1%>
      <th><%= @tableHeadings[i] %></th>
    <% end %>    
    <% @rows.each do |row| %>
      <tr>
        <% for j in 0..@tableHeadings.length-1 %>        
          <td><%= row[@tableHeadings[j]] %> </td>
        <% end %>  
      </tr>       
    <% end %>    
</table> 
